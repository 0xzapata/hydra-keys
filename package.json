{
  "name": "hydra-keys",
  "version": "0.1.0",
  "description": "Secure, extensible CLI for managing API keys across multiple providers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hydra-keys": "./bin/run"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "postbuild": "oclif manifest && oclif readme",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepack": "npm run build",
    "pretest": "npm run build"
  },
  "keywords": [
    "api-key",
    "key-management",
    "openrouter",
    "convex",
    "cli",
    "oclif"
  ],
  "author": "Your Name",
  "license": "MIT",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^4.0.0",
    "@oclif/plugin-help": "^6.0.0",
    "@oclif/plugin-not-found": "^3.0.0",
    "@oclif/plugin-plugins": "^4.0.0",
    "@postman/node-keytar": "^7.9.3",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "inquirer": "^9.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@oclif/test": "^4.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^29.7.0",
    "oclif": "^4.0.0",
    "prettier": "^3.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "oclif": {
    "bin": "hydra-keys",
    "dirname": "hydra-keys",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "provider": {
        "description": "Manage API key providers"
      },
      "key": {
        "description": "Manage API keys"
      },
      "storage": {
        "description": "Manage secure storage"
      },
      "plugin": {
        "description": "Manage plugins"
      },
      "config": {
        "description": "Manage configuration"
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/hydra-keys.git"
  }
}
